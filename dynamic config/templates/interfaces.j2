#jinja2: lstrip_blocks: "True", trim_blocks: "True"
interfaces {
{% for interface in lag_interfaces %}
    {{ interface.name }} {
    {% if interface.description is defined %}
        description "{{ interface.description }} member of {{ interface.lag }}";
    {% endif %}
        gigether-options {
            802.3ad {{ interface.lag }};
        }
    }
{% endfor %}
}
